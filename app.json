{
  "expo": {
    "name": "入力ファーストメモ - 画面開いたらすぐに入力で爆速メモ",
    "slug": "inputfirstmemo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "inputfirstmemo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ryunosuke.inputfirstmemo",
      "infoPlist": {
        "CFBundleDisplayName": "入力ファースト",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.ryunosuke.inputfirstmemo",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-5685067595656519~8386092482",
          "iosAppId": "ca-app-pub-5685067595656519~8386092482"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "55929d5d-1323-4ade-875a-86ac5d32610a"
      }
    }
  }
}
